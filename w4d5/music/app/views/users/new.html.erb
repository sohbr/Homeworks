<h1 class="page-header">New User Sign Up</h1>

<a class='button' href="<%= new_session_url %>">Existing Users</a>


  <form class="form" action="<%= users_url %>" method="post">
    <input type="hidden" name="authenticity_token" value="<% form_authenticity_token %>">

    <br>

    <label for="new_user_email">Email</label>
    <input id="email" type="text" name="user[email]">

    <br>

    <label for="new_user_password">Password</label>
    <input id="email" type="password" name="user[password]">

    <br>

    <input type="submit" value="Submit">

  </form>
